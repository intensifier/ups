echo $ECHO_N "checking if uses ELF executables... $ECHO_C" >&6
ups_cv_sys_is_elf=no
case "$host" in
    *-*-solaris2*)	ups_cv_sys_is_elf=yes ;;
    *-*-linux*)		ups_cv_sys_is_elf=yes ;;
    *-*-freebsd*)
	if test -x /usr/bin/objformat
	then
	    case "`/usr/bin/objformat`" in
		elf) ups_cv_sys_is_elf=yes ;;
	    esac
	fi
	;;
esac
# Allow user override
# Check whether --enable-aout or --disable-aout was given.
if test "${enable_aout+set}" = set; then
  enableval="$enable_aout"

fi;
case "$enable_aout" in
    yes) ups_cv_sys_is_elf=no ;;
esac
# Check whether --enable-elf or --disable-elf was given.
if test "${enable_elf+set}" = set; then
  enableval="$enable_elf"

fi;
case "$enable_elf" in
    yes) ups_cv_sys_is_elf=yes ;;
esac
