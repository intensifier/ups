
# Holds UPS specific targets

HDRS	= \
	cmplrs/dwarf_addr_finder.h \
	dwarf.h dwarf_*.h libdwarf*.h pro_*.h

SOURCE	= dwarf_*.c pro_*.c

# Files needed by AutoConf and configure
CONF	= \
	Makefile.in configure.in config.h.in UpsMakefile.in \
	acconfig.h configure config.guess config.sub install.sh

DOCS	= *.mm *.ps

OTHERS	= \
	CHANGES COPYING ChangeLog LIBDWARFCOPYRIGHT NEWS README \
	bldDWindex.sh

EDITED	= *.dwarflib

FILES	= ${HDRS} ${SOURCE} ${CONF} ${DOCS} ${OTHERS} ${EDITED}

# List one to a line the files in this directory that should go in a release
list:
	@echo ${FILES} | tr ' ' '\012'

check-list:
	@-bash -c "diff -bc <(make -f UpsMakefile list | sort) <(ls | sort)"

diffs:
	@-for i in ${EDITED}; do \
		diff -bc $$i `basename $$i .dwarflib`; \
	done

